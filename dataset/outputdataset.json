{
	"name": "outputdataset",
	"properties": {
		"linkedServiceName": {
			"referenceName": "linkedservicehari",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"directory": {
				"type": "string",
				"defaultValue": "output"
			},
			"filename": {
				"type": "string",
				"defaultValue": "sample.txt"
			}
		},
		"annotations": [],
		"type": "Binary",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@dataset().filename",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@dataset().directory",
					"type": "Expression"
				},
				"container": "adfdemo"
			}
		}
	}
}